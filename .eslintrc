{
  "extends": [
    "@readme/eslint-config",
    "plugin:compat/recommended"
  ],
  "root": true,
  "rules": {
    "global-require": "off"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "globalThis": true
  },
  "settings": {
    "polyfills": [
      "fetch",
      "Headers",
      "Request"
    ]
  }
}
