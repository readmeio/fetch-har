    "formdata-node": "^4.3.2"
  },
  "devDependencies": {
    "@jsdevtools/host-environment": "^2.1.2",
    "@jsdevtools/karma-config": "^3.1.7",
    "@readme/eslint-config": "^10.4.5",
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.15",
    "@types/mocha": "^10.0.0",
    "@types/multer": "^1.4.7",
