{
  "name": "fetch-har",
  "version": "9.0.0",
  "description": "Make a fetch request from a HAR definition",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
