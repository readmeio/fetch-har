{
  "name": "fetch-har",
  "version": "11.1.1",
  "description": "Make a fetch request from a HAR definition",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "module": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "npm run prettier:write && npx biome check --write",
    "lint": "npm run lint:types && npm run lint:ts && npm run prettier",
    "lint:ts": "biome check",
    "lint:types": "tsc",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --check --write .",
    "test": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/readmeio/fetch-har.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/readmeio/fetch-har/issues"
  },
  "homepage": "https://github.com/readmeio/fetch-har#readme",
  "dependencies": {
    "@readme/data-urls": "^3.0.0",
    "@types/har-format": "^1.2.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@jsdevtools/host-environment": "^2.1.2",
    "@readme/standards": "^2.2.0",
    "@types/express": "^5.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.1.4",
    "datauri": "^4.1.0",
    "express": "^5.1.0",
    "fetch-mock": "^12.2.1",
    "har-examples": "^4.0.2",
    "msw": "^2.8.4",
    "multer": "^1.4.5-lts.1",
    "nock": "^14.0.0",
    "prettier": "^3.0.3",
    "tsup": "^8.5.0",
    "typescript": "^5.2.2",
    "vitest": "^3.1.4"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/standards/prettier"
}
