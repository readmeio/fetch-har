{
  "extends": ["@readme/eslint-config", "plugin:compat/recommended"],
  "root": true,
  "env": {
    "browser": true
  },
  "globals": {
    "globalThis": true
  },
  "rules": {
    "global-require": "off",
    "no-case-declarations": "off"
  },
  "settings": {
    "polyfills": [
      "fetch",
      "Headers",
      "Request"
    ]
  }
}
