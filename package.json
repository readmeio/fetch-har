    "prepack": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "vitest --coverage",
    "test:browser": "vitest --browser.name=chrome --browser.headless"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/fetch-har.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/readmeio/fetch-har/issues"
  },
  "homepage": "https://github.com/readmeio/fetch-har#readme",
  "dependencies": {
    "@readme/data-urls": "^1.0.1",
    "@types/har-format": "^1.2.11",
    "readable-stream": "^3.6.0"
  },
  "optionalDependencies": {
    "formdata-node": "^4.3.2"
  },
  "devDependencies": {
    "@jsdevtools/host-environment": "^2.1.2",
    "@readme/eslint-config": "^12.0.2",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.3",
    "@types/readable-stream": "^2.3.15",
    "@vitest/browser": "^0.34.1",
    "@vitest/coverage-v8": "^0.34.1",
    "datauri": "^4.1.0",
    "eslint": "^8.44.0",
    "express": "^4.18.1",
    "fetch-mock": "^9.11.0",
    "form-data": "^4.0.0",
    "form-data-encoder": "^1.7.1",
    "formdata-node": "^4.3.2",
    "har-examples": "^3.1.1",
    "isomorphic-fetch": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nock": "^13.3.0",
    "node-fetch": "^2.6.0",
    "prettier": "^3.0.1",
    "temp-dir": "^2.0.0",
    "typescript": "^5.1.6",
    "undici": "^5.22.1",
    "vitest": "^0.34.1",
    "webdriverio": "^8.14.3"
