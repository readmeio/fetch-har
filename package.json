{
  "name": "fetch-har",
  "version": "4.0.3",
  "description": "Make a fetch request from a HAR file",
  "main": "index.js",
  "engines": {
    "node": "^12 || ^14 || ^16"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**.js\"",
    "release": "conventional-changelog -i CHANGELOG.md -s",
    "serve": "node __tests__/server.js",
    "test": "jest --coverage"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/fetch-har.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/readmeio/fetch-har/issues"
  },
  "homepage": "https://github.com/readmeio/fetch-har#readme",
  "dependencies": {
    "parse-data-url": "^4.0.1"
  },
  "devDependencies": {
    "@readme/eslint-config": "^6.0.0",
    "body-parser": "^1.19.0",
    "conventional-changelog-cli": "^2.0.31",
    "cookie-parser": "^1.4.5",
    "eslint": "^7.31.0",
    "eslint-plugin-compat": "^3.11.1",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "har-examples": "^2.0.1",
    "jest": "^27.0.3",
    "jest-puppeteer": "^5.0.3",
    "multer": "^1.4.2",
    "nock": "^13.1.1",
    "node-fetch": "^2.6.0",
    "prettier": "^2.3.2",
    "puppeteer": "^9.0.0",
    "webpack": "^5.47.1",
    "webpack-dev-middleware": "^5.0.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/eslint-config/prettier",
  "jest": {
    "preset": "jest-puppeteer",
    "globals": {
      "SERVER_URL": "http://localhost:4444"
    },
    "testMatch": [
      "<rootDir>/__tests__/*.test.js"
    ]
  }
}
